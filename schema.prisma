datasource db {
  provider = "sqlite"
  url      = "file:./data.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int     @id @default(autoincrement())
  session String
  link_visited String?
  name String?
}

model Link {
  id    Int     @id @default(autoincrement())
  url String  @unique
  map_url String
  title String
  body String
  thumbnail_link String
  routing String
  tl1 String
  tl2 String?
  tl3 String?
  tl4 String?
  tl5 String?
  owner User? @relation(fields: [ownerId], references: [id])
  ownerId Int?
}